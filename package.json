{
  "name": "reach",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node -r dotenv/config server",
    "pm2": "pm2 start npm --name \"REACH_WEB\" -- start",
    "restart": "rm -rf node_modules && yarn && yarn build && pm2 restart REACH_WEB",
    "restart-all": "yarn build && pm2 restart all"
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@types/react-places-autocomplete": "^7.2.9",
    "antd": "^5.7.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "country-codes-list": "^1.6.11",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "https-proxy-agent": "6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-helmet": "^6.1.0",
    "react-markdown": "^9.0.1",
    "react-pin-field": "^3.1.3",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "react-share": "^4.4.1",
    "rehype-external-links": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.5",
    "yt-channel-info": "^3.2.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.37",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
